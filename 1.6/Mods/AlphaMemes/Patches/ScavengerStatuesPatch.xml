<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs</xpath>
		<value>
			<DesignatorDropdownGroupDef>
				<defName>AP_ScavengerFakeSculptures</defName>
				<label>scavenger fake sculptures</label>
			</DesignatorDropdownGroupDef>
			<ThingDef Name="AP_SmallScavengerSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>small scavenger sculpture</label>
				<description>A scavenger style torso-sized piece of material sculpted into an artistic form. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<fillPercent>0.35</fillPercent>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
				</graphicData>
				<dominantStyleCategory>AM_Scavenger</dominantStyleCategory>
				<rotatable>false</rotatable>
				<designatorDropdown>AP_ScavengerFakeSculptures</designatorDropdown>
				<statBases>
					<MaxHitPoints>90</MaxHitPoints>
					<Mass>3</Mass>
					<Beauty>50</Beauty>
					<WorkToMake>18000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">10</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_SmallScavengerSculptureBase">
				<defName>AP_ScavengerFakeSculptureSmallA</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Scavenger/Sculptures/Small/AM_SculptureSmallScavengerA</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_SmallScavengerSculptureBase">
				<defName>AP_ScavengerFakeSculptureSmallB</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Scavenger/Sculptures/Small/AM_SculptureSmallScavengerB</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_SmallScavengerSculptureBase">
				<defName>AP_ScavengerFakeSculptureSmallC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Scavenger/Sculptures/Small/AM_SculptureSmallScavengerC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_MediumScavengerFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>large scavenger sculpture</label>
				<description>A scavenger style person-sized piece of material sculpted into an artistic form. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<castEdgeShadows>true</castEdgeShadows>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
					<drawSize>(3,3)</drawSize>
				</graphicData>
				<dominantStyleCategory>AM_Scavenger</dominantStyleCategory>
				<designatorDropdown>AP_ScavengerFakeSculptures</designatorDropdown>
				<fillPercent>0.5</fillPercent>
				<rotatable>false</rotatable>
				<statBases>
					<MaxHitPoints>150</MaxHitPoints>
					<Mass>7</Mass>
					<Beauty>100</Beauty>
					<WorkToMake>30000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">20</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_MediumScavengerFakeSculptureBase">
				<defName>AP_ScavengerFakeSculptureLargeA</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Scavenger/Sculptures/Large/AM_SculptureLargeScavengerA</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_MediumScavengerFakeSculptureBase">
				<defName>AP_ScavengerFakeSculptureLargeB</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Scavenger/Sculptures/Large/AM_SculptureLargeScavengerB</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_MediumScavengerFakeSculptureBase">
				<defName>AP_ScavengerFakeSculptureLargeC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Scavenger/Sculptures/Large/AM_SculptureLargeScavengerC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_GrandScavengerFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>grand scavenger sculpture</label>
				<description>A scavenger style  huge sculpture sized to dominate a room. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<castEdgeShadows>true</castEdgeShadows>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
					<drawSize>(4,4)</drawSize>
				</graphicData>
				<designatorDropdown>AP_ScavengerFakeSculptures</designatorDropdown>
				<fillPercent>0.5</fillPercent>
				<rotatable>false</rotatable>
				<dominantStyleCategory>AM_Scavenger</dominantStyleCategory>
				<size>(2,2)</size>
				<canOverlapZones>false</canOverlapZones>
				<statBases>
					<MaxHitPoints>300</MaxHitPoints>
					<Beauty>400</Beauty>
					<Mass>28</Mass>
					<WorkToMake>105000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">40</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_GrandScavengerFakeSculptureBase">
				<defName>AP_ScavengerFakeSculptureGrandA</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Scavenger/Sculptures/Grand/AM_SculptureGrandScavengerA</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_GrandScavengerFakeSculptureBase">
				<defName>AP_ScavengerFakeSculptureGrandB</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Scavenger/Sculptures/Grand/AM_SculptureGrandScavengerB</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_GrandScavengerFakeSculptureBase">
				<defName>AP_ScavengerFakeSculptureGrandC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Scavenger/Sculptures/Grand/AM_SculptureGrandScavengerC</texPath>
				</graphicData>
			</ThingDef>
		</value>
	</Operation>
</Patch>