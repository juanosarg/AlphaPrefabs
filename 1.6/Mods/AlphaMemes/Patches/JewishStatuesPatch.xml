				</graphicData>
				<designatorDropdown>AP_JewishFakeSculptures</designatorDropdown>
				<fillPercent>0.5</fillPercent>
				<rotatable>false</rotatable>
				<dominantStyleCategory>AM_Jewish</dominantStyleCategory>
				<size>(2,2)</size>
				<canOverlapZones>false</canOverlapZones>
				<statBases>
					<MaxHitPoints>300</MaxHitPoints>
					<Beauty>400</Beauty>
					<Mass>28</Mass>
					<WorkToMake>105000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">40</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
