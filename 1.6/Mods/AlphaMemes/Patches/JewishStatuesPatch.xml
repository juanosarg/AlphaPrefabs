<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs</xpath>
		<value>
			<DesignatorDropdownGroupDef>
				<defName>AP_JewishFakeSculptures</defName>
				<label>jewish fake sculptures</label>
			</DesignatorDropdownGroupDef>
			<ThingDef Name="AP_SmallJewishSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>small jewish sculpture</label>
				<description>A jewish style torso-sized piece of material sculpted into an artistic form. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<fillPercent>0.35</fillPercent>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
				</graphicData>
				<dominantStyleCategory>AM_Jewish</dominantStyleCategory>
				<rotatable>false</rotatable>
				<designatorDropdown>AP_JewishFakeSculptures</designatorDropdown>
				<statBases>
					<MaxHitPoints>90</MaxHitPoints>
					<Mass>3</Mass>
					<Beauty>50</Beauty>
					<WorkToMake>18000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">10</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_SmallJewishSculptureBase">
				<defName>AP_JewishFakeSculptureSmallA</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Jewish/Sculptures/Small/AM_SculptureSmallJewishA</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_SmallJewishSculptureBase">
				<defName>AP_JewishFakeSculptureSmallB</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Jewish/Sculptures/Small/AM_SculptureSmallJewishB</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_SmallJewishSculptureBase">
				<defName>AP_JewishFakeSculptureSmallC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Jewish/Sculptures/Small/AM_SculptureSmallJewishC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_MediumJewishFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>large jewish sculpture</label>
				<description>A jewish style person-sized piece of material sculpted into an artistic form. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<castEdgeShadows>true</castEdgeShadows>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
					<drawSize>(3,3)</drawSize>
				</graphicData>
				<dominantStyleCategory>AM_Jewish</dominantStyleCategory>
				<designatorDropdown>AP_JewishFakeSculptures</designatorDropdown>
				<fillPercent>0.5</fillPercent>
				<rotatable>false</rotatable>
				<statBases>
					<MaxHitPoints>150</MaxHitPoints>
					<Mass>7</Mass>
					<Beauty>100</Beauty>
					<WorkToMake>30000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">20</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_MediumJewishFakeSculptureBase">
				<defName>AP_JewishFakeSculptureLargeA</defName>
