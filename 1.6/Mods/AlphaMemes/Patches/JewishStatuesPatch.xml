				<graphicData>
					<texPath>Things/Building/BuildingStyles/Jewish/Sculptures/Large/AM_SculptureLargeJewishC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_GrandJewishFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>grand jewish sculpture</label>
				<description>A jewish style  huge sculpture sized to dominate a room. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<castEdgeShadows>true</castEdgeShadows>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
