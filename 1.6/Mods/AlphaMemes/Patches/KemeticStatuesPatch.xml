<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs</xpath>
		<value>
			<DesignatorDropdownGroupDef>
				<defName>AP_KemeticFakeSculptures</defName>
				<label>kemetic fake sculptures</label>
			</DesignatorDropdownGroupDef>
			<ThingDef Name="AP_SmallKemeticSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>small kemetic sculpture</label>
				<description>A kemetic style torso-sized piece of material sculpted into an artistic form. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<fillPercent>0.35</fillPercent>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
				</graphicData>
				<dominantStyleCategory>AM_Kemetic</dominantStyleCategory>
				<rotatable>false</rotatable>
				<designatorDropdown>AP_KemeticFakeSculptures</designatorDropdown>
				<statBases>
					<MaxHitPoints>90</MaxHitPoints>
					<Mass>3</Mass>
					<Beauty>50</Beauty>
					<WorkToMake>18000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">10</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_SmallKemeticSculptureBase">
				<defName>AP_KemeticFakeSculptureSmallA</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Kemetic/Sculptures/Small/AM_SculptureSmallKemeticA</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_SmallKemeticSculptureBase">
				<defName>AP_KemeticFakeSculptureSmallB</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Kemetic/Sculptures/Small/AM_SculptureSmallKemeticB</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_SmallKemeticSculptureBase">
				<defName>AP_KemeticFakeSculptureSmallC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Kemetic/Sculptures/Small/AM_SculptureSmallKemeticC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_MediumKemeticFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>large kemetic sculpture</label>
				<description>A kemetic style person-sized piece of material sculpted into an artistic form. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<castEdgeShadows>true</castEdgeShadows>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
					<drawSize>(3,3)</drawSize>
				</graphicData>
				<dominantStyleCategory>AM_Kemetic</dominantStyleCategory>
				<designatorDropdown>AP_KemeticFakeSculptures</designatorDropdown>
				<fillPercent>0.5</fillPercent>
				<rotatable>false</rotatable>
				<statBases>
					<MaxHitPoints>150</MaxHitPoints>
					<Mass>7</Mass>
					<Beauty>100</Beauty>
					<WorkToMake>30000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">20</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_MediumKemeticFakeSculptureBase">
				<defName>AP_KemeticFakeSculptureLargeA</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Kemetic/Sculptures/Large/AM_SculptureLargeKemeticA</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_MediumKemeticFakeSculptureBase">
				<defName>AP_KemeticFakeSculptureLargeB</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Kemetic/Sculptures/Large/AM_SculptureLargeKemeticB</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_MediumKemeticFakeSculptureBase">
				<defName>AP_KemeticFakeSculptureLargeC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Kemetic/Sculptures/Large/AM_SculptureLargeKemeticC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_GrandKemeticFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>grand kemetic sculpture</label>
				<description>A kemetic style  huge sculpture sized to dominate a room. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<castEdgeShadows>true</castEdgeShadows>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
					<drawSize>(4,4)</drawSize>
				</graphicData>
				<designatorDropdown>AP_KemeticFakeSculptures</designatorDropdown>
				<fillPercent>0.5</fillPercent>
				<rotatable>false</rotatable>
				<dominantStyleCategory>AM_Kemetic</dominantStyleCategory>
				<size>(2,2)</size>
				<canOverlapZones>false</canOverlapZones>
				<statBases>
					<MaxHitPoints>300</MaxHitPoints>
					<Beauty>400</Beauty>
					<Mass>28</Mass>
					<WorkToMake>105000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">40</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_GrandKemeticFakeSculptureBase">
				<defName>AP_KemeticFakeSculptureGrandA</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Kemetic/Sculptures/Grand/AM_SculptureGrandKemeticA</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_GrandKemeticFakeSculptureBase">
				<defName>AP_KemeticFakeSculptureGrandB</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Kemetic/Sculptures/Grand/AM_SculptureGrandKemeticB</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_GrandKemeticFakeSculptureBase">
				<defName>AP_KemeticFakeSculptureGrandC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Kemetic/Sculptures/Grand/AM_SculptureGrandKemeticC</texPath>
				</graphicData>
			</ThingDef>
		</value>
	</Operation>
