					<texPath>Things/Building/BuildingStyles/Neolithic/Sculptures/Large/AM_SculptureLargeNeolithicC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_GrandNeolithicFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
