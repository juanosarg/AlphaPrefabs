					<texPath>Things/Building/BuildingStyles/Neolithic/Sculptures/Small/AM_SculptureSmallNeolithicB</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_SmallNeolithicSculptureBase">
				<defName>AP_NeolithicFakeSculptureSmallC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Neolithic/Sculptures/Small/AM_SculptureSmallNeolithicC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_MediumNeolithicFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>large neolithic sculpture</label>
				<description>A neolithic style person-sized piece of material sculpted into an artistic form. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<castEdgeShadows>true</castEdgeShadows>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
					<drawSize>(3,3)</drawSize>
				</graphicData>
				<dominantStyleCategory>AM_Neolithic</dominantStyleCategory>
				<designatorDropdown>AP_NeolithicFakeSculptures</designatorDropdown>
				<fillPercent>0.5</fillPercent>
				<rotatable>false</rotatable>
				<statBases>
					<MaxHitPoints>150</MaxHitPoints>
					<Mass>7</Mass>
					<Beauty>100</Beauty>
					<WorkToMake>30000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">20</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
