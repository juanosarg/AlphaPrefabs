<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs</xpath>
		<value>
			<DesignatorDropdownGroupDef>
				<defName>AP_SteampunkFakeSculptures</defName>
				<label>steampunk fake sculptures</label>
			</DesignatorDropdownGroupDef>
			<ThingDef Name="AP_SmallSteampunkSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>small steampunk sculpture</label>
				<description>A steampunk style torso-sized piece of material sculpted into an artistic form. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<fillPercent>0.35</fillPercent>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
				</graphicData>
				<dominantStyleCategory>AM_SteampunkRevival</dominantStyleCategory>
				<rotatable>false</rotatable>
				<designatorDropdown>AP_SteampunkFakeSculptures</designatorDropdown>
				<statBases>
					<MaxHitPoints>90</MaxHitPoints>
					<Mass>3</Mass>
					<Beauty>50</Beauty>
					<WorkToMake>18000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">10</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_SmallSteampunkSculptureBase">
				<defName>AP_SteampunkFakeSculptureSmallA</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Steampunk/Sculptures/Small/AM_SculptureSmallSteampunkA</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_SmallSteampunkSculptureBase">
				<defName>AP_SteampunkFakeSculptureSmallB</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Steampunk/Sculptures/Small/AM_SculptureSmallSteampunkB</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_SmallSteampunkSculptureBase">
				<defName>AP_SteampunkFakeSculptureSmallC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Steampunk/Sculptures/Small/AM_SculptureSmallSteampunkC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_MediumSteampunkFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>large steampunk sculpture</label>
				<description>A steampunk style person-sized piece of material sculpted into an artistic form. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<castEdgeShadows>true</castEdgeShadows>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
					<drawSize>(3,3)</drawSize>
				</graphicData>
				<dominantStyleCategory>AM_SteampunkRevival</dominantStyleCategory>
				<designatorDropdown>AP_SteampunkFakeSculptures</designatorDropdown>
				<fillPercent>0.5</fillPercent>
				<rotatable>false</rotatable>
				<statBases>
					<MaxHitPoints>150</MaxHitPoints>
					<Mass>7</Mass>
					<Beauty>100</Beauty>
					<WorkToMake>30000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">20</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_MediumSteampunkFakeSculptureBase">
				<defName>AP_SteampunkFakeSculptureLargeA</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Steampunk/Sculptures/Large/AM_SculptureLargeSteampunkA</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_MediumSteampunkFakeSculptureBase">
				<defName>AP_SteampunkFakeSculptureLargeB</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Steampunk/Sculptures/Large/AM_SculptureLargeSteampunkB</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef ParentName="AP_MediumSteampunkFakeSculptureBase">
				<defName>AP_SteampunkFakeSculptureLargeC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Steampunk/Sculptures/Large/AM_SculptureLargeSteampunkC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_GrandSteampunkFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>grand steampunk sculpture</label>
				<description>A steampunk style  huge sculpture sized to dominate a room. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<castEdgeShadows>true</castEdgeShadows>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
					<drawSize>(4,4)</drawSize>
				</graphicData>
				<designatorDropdown>AP_SteampunkFakeSculptures</designatorDropdown>
				<fillPercent>0.5</fillPercent>
				<rotatable>false</rotatable>
				<dominantStyleCategory>AM_SteampunkRevival</dominantStyleCategory>
				<size>(2,2)</size>
				<canOverlapZones>false</canOverlapZones>
				<statBases>
					<MaxHitPoints>300</MaxHitPoints>
					<Beauty>400</Beauty>
					<Mass>28</Mass>
					<WorkToMake>105000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">40</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_GrandSteampunkFakeSculptureBase">
				<defName>AP_SteampunkFakeSculptureGrandA</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Steampunk/Sculptures/Grand/AM_SculptureGrandSteampunkA</texPath>
				</graphicData>
			</ThingDef>
			
		</value>
	</Operation>
</Patch>