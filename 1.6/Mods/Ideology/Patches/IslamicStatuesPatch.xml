			<ThingDef ParentName="AP_SmallIslamicSculptureBase">
				<defName>AP_IslamicFakeSculptureSmallC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Islamic/Sculptures/Small/SculptureSmallIslamicC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_MediumIslamicFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
