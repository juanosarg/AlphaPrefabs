<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs</xpath>
		<value>
			<DesignatorDropdownGroupDef>
				<defName>AP_AnimalistFakeSculptures</defName>
