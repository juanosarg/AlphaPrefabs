<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs</xpath>
		<value>
			<DesignatorDropdownGroupDef>
				<defName>AP_CorporateFakeSculptures</defName>
				<label>corporate fake sculptures</label>
			</DesignatorDropdownGroupDef>
			<ThingDef Name="AP_SmallCorporateSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>small corporate sculpture</label>
				<description>A corporate style torso-sized piece of material sculpted into an artistic form. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<fillPercent>0.35</fillPercent>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
				</graphicData>
				<dominantStyleCategory>VME_Corporate</dominantStyleCategory>
				<rotatable>false</rotatable>
				<designatorDropdown>AP_CorporateFakeSculptures</designatorDropdown>
				<statBases>
					<MaxHitPoints>90</MaxHitPoints>
					<Mass>3</Mass>
					<Beauty>50</Beauty>
					<WorkToMake>18000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">10</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_SmallCorporateSculptureBase">
				<defName>AP_CorporateFakeSculptureSmallA</defName>
