			</ThingDef>
			<ThingDef ParentName="AP_SmallEsotericismSculptureBase">
				<defName>AP_EsotericismFakeSculptureSmallC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Esotericism/Sculptures/Small/VME_SculptureSmallEsotericismC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_MediumEsotericismFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
