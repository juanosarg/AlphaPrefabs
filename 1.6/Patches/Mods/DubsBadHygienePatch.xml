<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dubs Bad Hygiene</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_SmallBedroomWithBathroom"]/researchPrerequisites</xpath>
					<value>
						<li>Plumbing</li>
						<li>ModernFixtures</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_Bedroom_Bath"]/researchPrerequisites</xpath>
					<value>
						<li>Plumbing</li>
						<li>ModernFixtures</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_FullHouse_NobleMansion"]/researchPrerequisites</xpath>
					<value>
						<li>Plumbing</li>
						<li>ModernFixtures</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

