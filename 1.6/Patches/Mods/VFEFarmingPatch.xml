<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Furniture Expanded - Farming</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_Nature_2"]/researchPrerequisites</xpath>
					<value>
						<li>VFE_Res_AdvancedHydroponics</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_SmallEcosystemBay"]/researchPrerequisites</xpath>
					<value>
						<li>VFE_Res_AdvancedHydroponics</li>
						<li>VFE_Res_Sprinkler</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

