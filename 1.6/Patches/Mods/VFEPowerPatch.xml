<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Furniture Expanded - Power</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_Power_SolarFarm"]/researchPrerequisites</xpath>
					<value>
						<li>VFE_AdvancedPowerSources</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_Research_1"]/researchPrerequisites</xpath>
					<value>
						<li>VFE_NuclearPower</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_PowerAdvanced"]/researchPrerequisites</xpath>
					<value>
						<li>VFE_AdvancedPowerSources</li>
						<li>VFE_AdvancedBatteries</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_DualNuclearReactor"]/researchPrerequisites</xpath>
					<value>
						<li>VFE_NuclearPower</li>
						<li>VFE_AdvancedBatteries</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

