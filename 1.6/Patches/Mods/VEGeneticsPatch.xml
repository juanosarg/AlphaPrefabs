<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Genetics Expanded</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>

			<li Class="PatchOperationAdd">
				<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_HugeGeneticsLab"]/researchPrerequisites</xpath>
				<value>
					<li>GR_GeneticEngineering</li>
					<li>GR_GeneticDuplication</li>
					<li>GR_GeneticAlteration</li>
					<li>GR_HybridImplantology</li>
					<li>GR_GeneticMechahybridization</li>
					
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_SmallGeneticsLab"]/researchPrerequisites</xpath>
				<value>
					<li>GR_GeneticEngineering</li>
					<li>GR_GeneticDuplication</li>
					<li>GR_GeneticAlteration</li>
					<li>GR_HybridImplantology</li>
				
				</value>
			</li>




		</operations>
		</match>
	</Operation>


</Patch>

