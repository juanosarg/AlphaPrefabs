<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dubs Rimatomics</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_RimatomicsResearch"]/researchPrerequisites</xpath>
					<value>
						<li>RimatomicsActivate</li>
						
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_LiberatorComplex"]/researchPrerequisites</xpath>
					<value>
						<li>RimatomicsActivate</li>
						
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

