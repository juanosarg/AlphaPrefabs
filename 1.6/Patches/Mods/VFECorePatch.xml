<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Furniture Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_Elegant_Bedroom_1"]/researchPrerequisites</xpath>
					<value>
						<li>MF_WasteDisposal</li>
						<li>MF_ModernFurniture</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_Elegant_Bedroom_2"]/researchPrerequisites</xpath>
					<value>
						<li>MF_WasteDisposal</li>
						<li>MF_ModernFurniture</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_FullHouse_Suburban"]/researchPrerequisites</xpath>
					<value>
						<li>MF_WasteDisposal</li>
						<li>MF_ModernFurniture</li>
						<li>MF_BasicFurniture</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

