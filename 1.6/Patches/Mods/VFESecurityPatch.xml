<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Furniture Expanded - Security</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_FortificationArtillery"]/researchPrerequisites</xpath>
					<value>
						<li>VFES_Artillery</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_TrenchLine"]/researchPrerequisites</xpath>
					<value>
						<li>VWE_TrenchWarfare</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_MedievalDefensiveLine"]/researchPrerequisites</xpath>
					<value>
						<li>VFES_SiegeEquipment</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_ChargeComplex"]/researchPrerequisites</xpath>
					<value>
						<li>VFES_RepulsorTechnology</li>
						<li>VFES_PulseChargedDefenses</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_BallistaLine"]/researchPrerequisites</xpath>
					<value>
						<li>VFES_SiegeEquipment</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_CatapultLine"]/researchPrerequisites</xpath>
					<value>
						<li>VFES_SiegeEquipment</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AlphaPrefabs.PrefabDef[defName = "AP_Prefab_SiegeNestBig"]/researchPrerequisites</xpath>
					<value>
						<li>VFES_Artillery</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

