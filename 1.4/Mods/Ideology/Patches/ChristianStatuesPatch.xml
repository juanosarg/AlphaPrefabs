			<ThingDef ParentName="AP_MediumChristianFakeSculptureBase">
				<defName>AP_ChristianFakeSculptureLargeC</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Christian/Sculptures/Large/SculptureLargeChristianC</texPath>
				</graphicData>
			</ThingDef>
			<ThingDef Name="AP_GrandChristianFakeSculptureBase" ParentName="AP_FakeSculptureBase" Abstract="True">
				<label>grand christian sculpture</label>
				<description>An christian style  huge sculpture sized to dominate a room. This sculpture seems to be mass produced and hence its value is significantly lower than usual.</description>
				<castEdgeShadows>true</castEdgeShadows>
				<graphicData>
					<graphicClass>Graphic_Single</graphicClass>
					<drawSize>(4,4)</drawSize>
				</graphicData>
				<designatorDropdown>AP_ChristianFakeSculptures</designatorDropdown>
				<fillPercent>0.5</fillPercent>
				<rotatable>false</rotatable>
				<dominantStyleCategory>Christian</dominantStyleCategory>
				<size>(2,2)</size>
				<canOverlapZones>false</canOverlapZones>
				<statBases>
					<MaxHitPoints>300</MaxHitPoints>
					<Beauty>400</Beauty>
					<Mass>28</Mass>
					<WorkToMake>105000</WorkToMake>
					<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">40</StyleDominance>
				</statBases>
				<stuffCategories>
					<li>Metallic</li>
					<li>Woody</li>
					<li>Stony</li>
				</stuffCategories>
				<costStuffCount>1</costStuffCount>
			</ThingDef>
			<ThingDef ParentName="AP_GrandChristianFakeSculptureBase">
				<defName>AP_ChristianFakeSculptureGrandA</defName>
				<graphicData>
					<texPath>Things/Building/BuildingStyles/Christian/Sculptures/Grand/SculptureGrandChristianA</texPath>
